  FormBox[
   SubscriptBox[
    RowBox[{"(", 
     RowBox[{
      RowBox[{"s", "(", "n", ")"}], " ", "mod", " ", 
      SuperscriptBox["p", "\[Alpha]"]}], ")"}], 
    RowBox[{"n", "\[GreaterEqual]", "0"}]], TraditionalForm]],ExpressionUUID->
  "8431cf87-a106-4858-9e25-9ca676c059f4"],
