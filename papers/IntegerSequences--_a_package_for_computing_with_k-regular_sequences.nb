  FormBox[
   SubscriptBox[
    RowBox[{"s", "(", "n", ")"}], 
    RowBox[{"n", "\[GreaterEqual]", "0"}]], TraditionalForm]],ExpressionUUID->
  "c1e4c7af-c28e-4725-aa3f-24fdf3becef6"],
