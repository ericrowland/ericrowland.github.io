  FormBox[
   RowBox[{"g", "(", 
    RowBox[{"0", ",", "\[Ellipsis]", ",", "0"}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0118d0b7-b81a-4de9-9b78-0961333caf4e"],
