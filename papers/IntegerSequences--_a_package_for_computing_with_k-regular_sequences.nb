Cell[TextData[{
 "Computing automata for sequences modulo ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["p", "\[Alpha]"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6ebf092f-81b1-4c27-9ae6-071ede6bf4ec"],
 " provides routine proofs of many congruences that were established in the \
literature by nontrivial case analyses. For example, Eu, Liu, and Yeh [4] \
proved that no Motzkin number is divisible by ",
 Cell[BoxData[
  FormBox["8", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b6ecf80a-09d7-45f4-94d0-15322b6d61a3"],
 ". The following computation proves this in less than a second. The \
resulting automaton has ",
 Cell[BoxData[
  FormBox["24", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "785c0bf7-7169-4461-9e46-dc32211d2eed"],
 " states."
}], "Text",ExpressionUUID->"1f75801a-864f-4ccc-aa39-c47d8bb6dcbc"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"automaton", "=", 
   RowBox[{"AutomaticSequenceAutomaton", "[", 
    RowBox[{"AutomaticSequenceReduce", "[", 
     RowBox[{
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{"MotzkinNumber", "[", "n", "]"}], ",", "8"}], "]"}], ",", 
      "n"}], "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"AutomatonStateCount", "[", "automaton", 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Sort", "[", 
  RowBox[{"AutomatonOutputAlphabet", "[", "automaton", "]"}], "]"}]}], "Input",\

 CellLabel->"In[18]:=",ExpressionUUID->"e5814d01-a34f-425a-a719-44c546ab5bbf"],

