Cell[TextData[{
 "Detecting fractional powers in a word is equivalent to determining equality \
of certain factors.  ",
 StyleBox["PowerFreeMorphismObstructions", "Input"],
 " uses ",
 StyleBox["SymbolicWordUnequal", "Input"],
 " to prove that certain symbolic morphisms preserve the property of being ",
 Cell[BoxData[
  FormBox[
   FractionBox["a", "b"], TraditionalForm]]],
 "-power-free and hence generate infinite ",
 Cell[BoxData[
  FormBox[
   FractionBox["a", "b"], TraditionalForm]]],
 "-power-free words:"
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"PowerFreeMorphismObstructions", "[", 
  RowBox[{
   RowBox[{"n_", "\[RuleDelayed]", 
    RowBox[{"SymbolicWord", "[", 
     RowBox[{
      TemplateBox[{"0",RowBox[{"a", "-", "1"}]},
       "Superscript"], ",", 
      TemplateBox[{"1","1"},
       "Superscript"], ",", 
      TemplateBox[{"0",RowBox[{"a", "-", "b", "-", "1"}]},
       "Superscript"], ",", 
      TemplateBox[{RowBox[{"n", "+", "1"}],"1"},
       "Superscript"]}], "]"}]}], ",", 
   FractionBox["a", "b"], ",", 
   RowBox[{
    RowBox[{"n", "\[GreaterEqual]", "0"}], "&&", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"a", "|", "b"}], ")"}], "\[Element]", "Integers"}], "&&", 
    RowBox[{
     RowBox[{"GCD", "[", 
      RowBox[{"a", ",", "b"}], "]"}], "\[Equal]", "1"}], "&&", 
    RowBox[{
     RowBox[{"GCD", "[", 
      RowBox[{"b", ",", "2"}], "]"}], "\[Equal]", "1"}], "&&", 
    RowBox[{
     FractionBox["5", "3"], "<", 
     FractionBox["a", "b"], "<", "2"}]}]}], "]"}]], "Input"],

Cell[BoxData[
 RowBox[{"{", "}"}]], "Output"]
}, Open  ]]
}, Closed]]
}, Open  ]],

