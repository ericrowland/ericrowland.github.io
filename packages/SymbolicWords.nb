Cell[TextData[{
 "Detecting fractional powers in a word is equivalent to determining equality \
of certain factors.  ",
 StyleBox["PowerFreeMorphismObstructions", "Input"],
 " uses ",
 StyleBox["SymbolicWordUnequal", "Input"],
 " to prove that certain symbolic morphisms preserve the property of being ",
 Cell[BoxData[
  FormBox[
   FractionBox["a", "b"], TraditionalForm]],ExpressionUUID->
  "b05b3637-cc03-48fe-8876-376155aec16d"],
 "-power-free and hence generate infinite ",
 Cell[BoxData[
  FormBox[
   FractionBox["a", "b"], TraditionalForm]],ExpressionUUID->
  "3136d678-d891-4ec4-b4e6-4c07ca2500f9"],
 "-power-free words:"
}], "Text",ExpressionUUID->"b9597c39-7d03-46ee-8adc-1003b2852f56"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"PowerFreeMorphismObstructions", "[", 
  RowBox[{
   RowBox[{"n_", "\[RuleDelayed]", 
    RowBox[{"SymbolicWord", "[", 
     RowBox[{
      TemplateBox[{"0",RowBox[{"a", "-", "1"}]},
       "Superscript"], ",", 
      TemplateBox[{"1","1"},
       "Superscript"], ",", 
      TemplateBox[{"0",RowBox[{"a", "-", "b", "-", "1"}]},
       "Superscript"], ",", 
      TemplateBox[{RowBox[{"n", "+", "1"}],"1"},
       "Superscript"]}], "]"}]}], ",", 
   FractionBox["a", "b"], ",", 
   RowBox[{
    RowBox[{"n", "\[GreaterEqual]", "0"}], "&&", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"a", "|", "b"}], ")"}], "\[Element]", "Integers"}], "&&", 
    RowBox[{
     RowBox[{"GCD", "[", 
      RowBox[{"a", ",", "b"}], "]"}], "\[Equal]", "1"}], "&&", 
    RowBox[{
     RowBox[{"GCD", "[", 
      RowBox[{"b", ",", "2"}], "]"}], "\[Equal]", "1"}], "&&", 
    RowBox[{
     FractionBox["5", "3"], "<", 
     FractionBox["a", "b"], "<", "2"}]}]}], "]"}]], "Input",ExpressionUUID->\
"3450ac24-7a91-429d-9992-7c3f7a6c7508"],

Cell[BoxData[
 RowBox[{"{", "}"}]], "Output",ExpressionUUID->"51ec50b0-b4e1-4f9a-ab96-247b5846569a"]
}, Open  ]]
}, Closed]]
