   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"DeleteFile", "[", 
         RowBox[{"FileNameJoin", "[", 
          RowBox[{"{", 
           RowBox[{
            StyleBox[
             RowBox[{"$", 
              StyleBox["TeX", "Input"], "FiguresDirectory"}]], ",", "#1"}], 
           "}"}], "]"}], "]"}], ";", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{
           StyleBox[
            RowBox[{"$", 
             StyleBox["TeX", "Input"], "FiguresDirectory"}]], ",", "#1"}], 
          "}"}], "]"}]}], ")"}], "&"}], ")"}], "/@", 
    RowBox[{"Select", "[", 
     RowBox[{
      RowBox[{"FileNameTake", "/@", 
       RowBox[{"FileNames", "[", 
        RowBox[{"___", ",", 
         StyleBox[
          RowBox[{"$", 
           StyleBox["TeX", "Input"], "FiguresDirectory"}]]}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"!", 
