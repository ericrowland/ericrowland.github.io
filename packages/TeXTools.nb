  RowBox[{"\"\<circle\>\"", ",", 
   RowBox[{"Graphics", "[", 
    RowBox[{"Circle", "[", "]"}], "]"}]}], "]"}]], "Input",ExpressionUUID->\
"4ba99dd3-680c-4b18-b27a-dbd350070b88"]
}, Closed]],

