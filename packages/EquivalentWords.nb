   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", "\<\"a\"\>", "}"}], ",", "\<\"B\"\>"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", "\<\"b\"\>", "}"}], ",", "\<\"a\"\>"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", "\<\"b\"\>", "}"}], ",", "\<\"A\"\>"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", "\<\"A\"\>", "}"}], ",", "\<\"b\"\>"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", "\<\"A\"\>", "}"}], ",", "\<\"B\"\>"}], "}"}], ",", 
