Cell[BoxData["\<\"abababbbaabbba\"\>"], "Output"]
}, Open  ]],

Cell[TextData[{
 "In ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "2"], TraditionalForm]]],
 ", it suffices to consider automorphisms where the set ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]]],
 " consists of a single generator.\n",
 StyleBox["$AllAutomorphisms", "Input"],
 " contains eight one-letter automorphisms:"
}], "Text"],

