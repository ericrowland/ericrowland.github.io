Cell[TextData[{
 "For the first five hundred ",
 Cell[BoxData[
  FormBox["2", TraditionalForm]]],
 "-color rules depending on ",
 Cell[BoxData[
  FormBox["4", TraditionalForm]]],
 " cells, determine which have row lengths that are not periodic in the range \
",
 Cell[BoxData[
  FormBox[
   RowBox[{"20", "\[LessEqual]", "t", "\[LessEqual]", "100"}], 
   TraditionalForm]]],
 ", and find the best-fit curves to their sequences of row lengths:"
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Cases", "[", 
  RowBox[{
   RowBox[{"Join", "@@", 
    RowBox[{"Thread", "/@", 
     RowBox[{"CreateRegularBoundaryData", "[", 
      RowBox[{"2", ",", "4", ",", 
       RowBox[{"\"\<FileName\>\"", "\[Rule]", "None"}], ",", 
       RowBox[{"\"\<RuleNumbers\>\"", "\[Rule]", 
        RowBox[{"Range", "[", 
         RowBox[{"0", ",", "500"}], "]"}]}], ",", 
